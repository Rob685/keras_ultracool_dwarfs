import numpy as np
import pandas as pd
import matplotlib.pyplot as plt

x = np.linspace(0,2*np.pi,0.001*np.pi)
y = np.linspace(0,np.pi/2,0.001*np.pi)

df = pd.DataFrame(x,y,columns = ['angle1','angle2'])

print(df)

plt.plot(df['angle1'],df.angle2,'k--')
plt.xlabel('angle1')
plt.ylabel('angle2')
plt.show()
